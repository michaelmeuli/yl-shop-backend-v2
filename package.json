    "@vendure/email-plugin": "2.0.3",
    "@vendure/job-queue-plugin": "^2.0.4",
    "bullmq": "1",
    "dotenv": "16.3.1",
    "pg": "8.11.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "concurrently": "8.2.0",
    "ts-node": "10.9.1"
